#!/bin/bash -x

[ -n "$vdpalog" ] || {
sudo sw/dpdk/build/app/dpdk-vfe-vdpa -a 0000:00:00.0 --log-level=.,8 -- --client --iface /tmp/vf- ;
exit 0;
}

loginfo dpdk-vfe-vdpa $pfslot $vfslot
# background run
tail -f /dev/null | \
sw/dpdk/build/app/dpdk-vfe-vdpa -a 0000:00:00.0 --log-level=.,8 -- --client --iface /tmp/vf- \
 >> $vdpalog 2>&1 &

